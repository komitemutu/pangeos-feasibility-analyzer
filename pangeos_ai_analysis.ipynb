    "# Pangeos yacht specifications\n",
    "pangeos_specs = {\n",
    "    'length': 550,           # meters\n",
    "    'beam': 200,             # meters\n",
    "    'displacement': 5000000, # tons\n",
    "    'propulsion_power': 22000, # kW\n",
    "    'material_cost': 8000000000, # USD (8 billion)\n",
    "    'construction_time': 84,  # months (7 years)\n",
    "    'crew_size': 600,\n",
    "    'passenger_capacity': 60000\n",
    "}\n",
    "\n",
    "print(\"🚢 Analyzing Pangeos Yacht Project...\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "# Multi-agent analysis\n",
    "agent_analysis = pangeos_ai.coordinate_analysis(pangeos_specs)\n",
    "\n",
    "print(f\"\\n🎯 Overall Feasibility: {agent_analysis['overall_feasibility']:.1f}%\")\n",
    "print(f\"📋 Recommendation: {agent_analysis['recommendation']}\")\n",
    "\n",
    "print(\"\\n🤖 Agent-by-Agent Analysis:\")\n",
    "for agent_name, result in agent_analysis['agent_results'].items():\n",
    "    print(f\"  {agent_name.capitalize()}: {result['feasibility_score']:.1f}%\")\n",
    "\n",
    "# Machine learning predictions\n",
    "pangeos_input = np.array([[pangeos_specs[col] for col in feature_columns]])\n",
    "pangeos_scaled = scaler_X.transform(pangeos_input)\n",
    "\n",
    "# Neural network prediction\n",
    "nn_pred_scaled = nn_model.predict(pangeos_scaled)\n",
    "nn_pred = scaler_y.inverse_transform(nn_pred_scaled)[0]\n",
    "\n",
    "# Ensemble predictions\n",
    "rf_pred = [rf_models[target].predict(pangeos_scaled)[0] for target in target_columns]\n",
    "gb_pred = [gb_models[target].predict(pangeos_scaled)[0] for target in target_columns]\n",
    "\n",
    "print(\"\\n🧠 Machine Learning Predictions:\")\n",
    "ml_results = pd.DataFrame({\n",
    "    'Aspect': target_columns,\n",
    "    'Neural Network': nn_pred,\n",
    "    'Random Forest': rf_pred,\n",
    "    'Gradient Boosting': gb_pred\n",
    "})\n",
    "\n",
    "# Calculate ensemble average\n",
    "ml_results['Ensemble Average'] = ml_results[['Neural Network', 'Random Forest', 'Gradient Boosting']].mean(axis=1)\n",
    "\n",
    "ml_results.round(1)"
