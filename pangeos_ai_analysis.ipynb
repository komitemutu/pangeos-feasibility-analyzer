    "def generate_pangeos_dataset(n_samples=2000):
    """Generate comprehensive dataset for Pangeos yacht analysis"""
    np.random.seed(42)
    
    # Base specifications with realistic variations
    data = {
        'length': np.random.normal(550, 80, n_samples),          # meters
        'beam': np.random.normal(200, 35, n_samples),            # meters  
        'displacement': np.random.normal(5000000, 800000, n_samples),  # tons
        'propulsion_power': np.random.normal(25000, 8000, n_samples),  # kW
        'material_cost': np.random.normal(6000000000, 1500000000, n_samples),  # USD
        'construction_time': np.random.normal(72, 18, n_samples),       # months
        'crew_size': np.random.normal(600, 120, n_samples),
        'passenger_capacity': np.random.normal(60000, 12000, n_samples)
    }
    
    df = pd.DataFrame(data)
    
    # Ensure positive values
    for col in df.columns:
        df[col] = np.abs(df[col])
    
    # Calculate multi-dimensional feasibility scores
    df['engineering_score'] = np.clip(
        80 - (df['displacement'] / 100000) + (df['propulsion_power'] / 1000) + np.random.normal(0, 5, n_samples), 
        0, 100
    )
    
    df['financial_score'] = np.clip(
        90 - (df['material_cost'] / 100000000) - (df['construction_time'] / 2) + np.random.normal(0, 8, n_samples),
        0, 100
    )
    
    df['environmental_score'] = np.clip(
        85 - (df['displacement'] / 150000) + np.random.normal(0, 6, n_samples),
        0, 100
    )
    
    df['logistics_score'] = np.clip(
        75 + (100000 / df['passenger_capacity']) - (df['construction_time'] / 3) + np.random.normal(0, 7, n_samples),
        0, 100
    )
    
    # Overall feasibility as weighted average
    df['overall_feasibility'] = (
        df['engineering_score'] * 0.35 +
        df['financial_score'] * 0.3 +
        df['environmental_score'] * 0.2 +
        df['logistics_score'] * 0.15
    )
    
    return df

# Generate the dataset
print("🔄 Generating Pangeos yacht dataset...")
