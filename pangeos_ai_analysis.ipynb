    "# Random Forest Ensemble\n",
    "rf_models = {}\n",
    "gb_models = {}\n",
    "\n",
    "for i, target in enumerate(target_columns):\n",
    "    print(f\"🌳 Training Random Forest for {target}...\")\n",
    "    \n",
    "    # Random Forest\n",
    "    rf_model = RandomForestRegressor(\n",
    "        n_estimators=200,\n",
    "        max_depth=15,\n",
    "        min_samples_split=5,\n",
    "        min_samples_leaf=2,\n",
    "        random_state=42,\n",
    "        n_jobs=-1\n",
    "    )\n",
    "    rf_model.fit(X_train_scaled, y_train[:, i])\n",
    "    rf_models[target] = rf_model\n",
    "    \n",
    "    # Gradient Boosting\n",
    "    gb_model = GradientBoostingRegressor(\n",
    "        n_estimators=150,\n",
    "        learning_rate=0.1,\n",
    "        max_depth=8,\n",
    "        random_state=42\n",
    "    )\n",
    "    gb_model.fit(X_train_scaled, y_train[:, i])\n",
    "    gb_models[target] = gb_model\n",
    "\n",
    "print(\"✅ Ensemble models trained successfully!\")"
   ]
  },
