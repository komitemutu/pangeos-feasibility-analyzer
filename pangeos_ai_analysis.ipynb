    "## 📊 Model Evaluation and Validation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Evaluate neural network\n",
    "nn_predictions_scaled = nn_model.predict(X_test_scaled)\n",
    "nn_predictions = scaler_y.inverse_transform(nn_predictions_scaled)\n",
    "\n",
    "# Evaluate ensemble models\n",
    "rf_predictions = np.zeros((len(X_test), len(target_columns)))\n",
    "gb_predictions = np.zeros((len(X_test), len(target_columns)))\n",
    "\n",
    "for i, target in enumerate(target_columns):\n",
    "    rf_predictions[:, i] = rf_models[target].predict(X_test_scaled)\n",
    "    gb_predictions[:, i] = gb_models[target].predict(X_test_scaled)\n",
    "\n",
    "# Calculate metrics\n",
    "def calculate_metrics(y_true, y_pred, model_name):\n",
    "    metrics = {}\n",
    "    for i, target in enumerate(target_columns):\n",
    "        mse = mean_squared_error(y_true[:, i], y_pred[:, i])\n",
    "        r2 = r2_score(y_true[:, i], y_pred[:, i])\n",
    "        metrics[target] = {'mse': mse, 'r2': r2}\n",
    "    \n",
    "    return metrics\n",
    "\n",
    "nn_metrics = calculate_metrics(y_test, nn_predictions, 'Neural Network')\n",
    "rf_metrics = calculate_metrics(y_test, rf_predictions, 'Random Forest')\n",
    "gb_metrics = calculate_metrics(y_test, gb_predictions, 'Gradient Boosting')\n",
    "\n",
    "# Display results\n",
    "results_df = pd.DataFrame({\n",
    "    'Target': target_columns,\n",
    "    'NN_R2': [nn_metrics[target]['r2'] for target in target_columns],\n",
    "    'RF_R2': [rf_metrics[target]['r2'] for target in target_columns],\n",
    "    'GB_R2': [gb_metrics[target]['r2'] for target in target_columns],\n",
    "    'NN_MSE': [nn_metrics[target]['mse'] for target in target_columns],\n",
    "    'RF_MSE': [rf_metrics[target]['mse'] for target in target_columns],\n",
    "    'GB_MSE': [gb_metrics[target]['mse'] for target in target_columns]\n",
    "})\n",
    "\n",
    "print(\"📊 Model Performance Comparison:\")\n",
    "results_df.round(4)"
   ]
